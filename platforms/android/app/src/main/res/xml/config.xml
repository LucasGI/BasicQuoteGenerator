<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cosoftlabs.basicquotegen" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Toast">
        <param name="android-package" value="nl.xservices.plugins.Toast" />
    </feature>
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <feature name="BackgroundMode">
        <param name="android-package" value="de.appplant.cordova.plugin.background.BackgroundMode" />
    </feature>
    <feature name="Badge">
        <param name="android-package" value="de.appplant.cordova.plugin.badge.Badge" />
    </feature>
    <name>Basic Quote Generator</name>
    <description>
        A basic quote generator.
    </description>
    <author email="jordan.olsen@cosoftstudios.com" href="">
        LucasGI
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon density="mdpi" src="www/res/ic_launcher/res/mdpi.png" />
    <icon density="hdpi" src="www/res/ic_launcher/res/hdpi.png" />
    <icon density="xhdpi" src="www/res/ic_launcher/res/xhdpi.png" />
    <icon density="xxhdpi" src="www/res/ic_launcher/res/xxhdpi.png" />
    <icon density="xxxhdpi" src="www/res/ic_launcher/res/xxxhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="KeepRunning" value="true" />
    <preference name="AndroidLaunchMode" value="singleInstance" />
    <preference name="Orientation" value="portrait" />
</widget>
